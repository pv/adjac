  impure elemental function abs_b(x) result(z)
    implicit none
    type(adjac_complex), intent(in) :: x
    type(adjac_double) :: z
    double precision :: v, dv_re, dv_im
    v = hypot(x%re%value, x%im%value)
    dv_re = x%re%value / v
    dv_im = x%im%value / v
    z = dv_re * x%re + dv_im * x%im
    z%value = v
  end function abs_b

